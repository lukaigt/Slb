# Solana RPC URL (mainnet) - REQUIRED
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Wallet private key (base58 string) - REQUIRED
PRIVATE_KEY=your_base58_private_key_here

# Jupiter API Key - REQUIRED (from portal.jup.ag)
JUPITER_API_KEY=your_jupiter_api_key_here

# Trade settings
TRADE_PERCENT=0.2
BUY_THRESHOLD=1
SELL_THRESHOLD=1
COOLDOWN_SECONDS=60

# Slippage in basis points (50 = 0.5%)
SLIPPAGE_BPS=50

# RPC commitment level
COMMITMENT=confirmed
